  "editor.acceptSuggestionOnEnter": "off", // 按回车时一定是真正的换行，只有tab才会接受Intellisense
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.snippetSuggestions": "inline", // snippets显示在补全列表顶端(top)，默认是inline
